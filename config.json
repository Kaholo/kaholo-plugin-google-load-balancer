{
  "name": "Google-Cloud-Load-Balancer",
  "type": "executer",
  "exposeRoute": false,
  "execProgram": "node",
  "main": "app.js",
  "imgUrl": "logo.png",
  "version": "0.0.1",
  "category": ["CLOUD"],
  "keywords": [
    "GCP",
    "compute",
    "google",
    "platform",
    "cloud",
    "project",
    "infra",
    "infrastructure",
    "network",
    "subnet",
    "firewall",
    "IP",
    "route"
  ],
  "settings": [
    {
      "name": "project",
      "viewName": "Default Project Name",
      "type": "string",
      "description": "GCP Project defines how your app interacts with services and resources",
      "placeholder": "project-alpha",
      "learnUrl": "https://cloud.google.com/resource-manager/docs/creating-managing-projects"
    }
  ],
  "methods": [
    {
      "name": "createLoadBalancer",
      "viewName": "Create Load Balancer",
      "params": [
        {
          "name": "creds",
          "viewName": "Service Account Credentials *",
          "required": false,
          "type": "vault",
          "description": "Vaulted JSON document containing service account credentials as provided by GCP",
          "learnUrl": "https://cloud.google.com/docs/authentication/production"
        },
        {
          "name": "project",
          "viewName": "Project *",
          "required": false,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listProjectsAuto",
          "description": "GCP Project defines how your app interacts with services and resources",
          "placeholder": "select using autocomplete",
          "learnUrl": "https://cloud.google.com/resource-manager/docs/creating-managing-projects"
        },
        {
          "name": "healthCheckName",
          "viewName": "HealthCheck name",
          "required": true,
          "type": "string",
          "description": "Name of the resource. Provided by the client when the resource is created. The name must be 1-63 characters long, and comply with RFC1035.",
          "placeholder": "kaholo-health-check",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/httpHealthChecks"
        },
        {
          "name": "type",
          "viewName": "HealthCheck type *",
          "required": true,
          "type": "string",
          "description": "Specifies the type of the healthCheck, either TCP, SSL, HTTP, HTTPS or HTTP2. Exactly one of the protocol-specific health check field must be specified, which must match type field.",
          "placeholder": "HTTP",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/httpHealthChecks"
        },
        {
          "name": "backendServiceName",
          "viewName": "Backend Service Name *",
          "required": true,
          "type": "string",
          "description": "Name of the resource. The name must be 1-63 characters long, and comply with RFC1035.",
          "placeholder": "kaholo-backend-service",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/backendServices/"
        },
        {
          "name": "zone",
          "viewName": "Zone *",
          "type": "autocomplete",
          "required": true,
          "autocompleteType": "function",
          "functionName": "listZonesAuto",
          "description": "Zone is a subdivision of GCP regions, typically region-a, b, or c",
          "placeholder": "select using autocomplete",
          "learnUrl": "https://cloud.google.com/compute/docs/regions-zones"
        },
        {
          "name": "instanceGroupName",
          "viewName": "Instance group URL",
          "required": true,
          "type": "autocomplete",
          "autocompleteType": "function",
          "functionName": "listInstanceGroupsAuto",
          "description": "URL of the instance group, e.g., https://www.googleapis.com/compute/v1/projects/project/zones/region/instanceGroups/instance-group ",
          "placeholder": "select using autocomplete",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/backendServices/"
        },
        {
          "name": "urlMapName",
          "viewName": "urlMapName",
          "required": true,
          "type": "string",
          "description": "Name of the resource. The name must be 1-63 characters long, and comply with RFC1035. ",
          "placeholder": "kaholo-url-map",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/urlMaps"
        },
        {
          "name": "httpProxyName",
          "viewName": "httpProxyName",
          "required": true,
          "type": "string",
          "description": "Name of the resource. The name must be 1-63 characters long, and comply with RFC1035. ",
          "placeholder": "kaholo-http-proxy",
          "learnUrl": "https://cloud.google.com/compute/docs/reference/rest/v1/targetHttpsProxies"
        }
      ]
    }
  ]
}
